{
  "manifest_version": 3,
  "name": "AdBlock",
  "description": "removes Youtube ad elements",
  "version": "1.1.2",
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://*.youtube.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
}

